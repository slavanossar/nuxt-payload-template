{
  "name": "payloadcms",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate": "cross-env NODE_OPTIONS=--no-deprecation pnpm run generate:importmap && cross-env NODE_OPTIONS=--no-deprecation pnpm run generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "regenerate-image-sizes": "cross-env payload run scripts/regenerate-image-sizes.ts",
    "seed": "cross-env payload run seed/index.ts",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "^5.1.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint-config-next": "^16.0.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.63.0",
    "@payloadcms/next": "^3.63.0",
    "@payloadcms/plugin-seo": "^3.63.0",
    "@payloadcms/richtext-lexical": "^3.63.0",
    "@payloadcms/storage-s3": "^3.63.0",
    "cross-env": "^10.1.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "next": "^15.5.6",
    "node-html-parser": "^7.0.1",
    "payload": "^3.63.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "0.34.5",
    "slugify": "^1.6.6"
  }
}
