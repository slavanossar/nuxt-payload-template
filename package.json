{
  "name": "nuxt3-payload-template",
  "private": true,
  "engine": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "serve": "node .output/server/index.mjs",
    "generate-types": "graphql-codegen -r dotenv/config --config codegen.ts && mv graphql/exports.d.ts graphql/index.js && node ./graphql/schemaQuery.mjs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.3",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.3",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@nuxtjs/apollo": "^5.0.0-alpha.4",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@pinia/nuxt": "^0.4.1",
    "@types/escape-html": "^1.0.2",
    "@vue-macros/nuxt": "^0.2.16",
    "@vueuse/nuxt": "^9.6.0",
    "dotenv": "^16.0.1",
    "escape-html": "^1.0.3",
    "nuxt": "^3.0.0",
    "pinia": "^2.0.16",
    "slate": "^0.88.1"
  }
}
