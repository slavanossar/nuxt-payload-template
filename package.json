{
  "name": "nuxt-payload-template",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "dotenv -- turbo run dev",
    "build": "dotenv -- turbo run build",
    "start": "dotenv -- turbo run start",
    "generate": "dotenv -- turbo run generate",
    "regenerate-image-sizes": "dotenv -- turbo run regenerate-image-sizes",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,md}\"",
    "update": "pnpm up --interactive --recursive",
    "update-latest": "pnpm up --interactive --latest --recursive",
    "configure-env": "configure-env"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "configure-env": "^1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "turbo": "^1.13.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "graphql": "^16.12.0",
    "prettier": "^3.6.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "ffmpeg-static",
      "sharp",
      "vue-demi"
    ]
  }
}
