{
  "name": "payload",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "npm run build:payload && npm run build:server",
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "@payloadcms/plugin-seo": "^1.0.13",
    "dashify": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "payload": "^1.15.6",
    "payload-blurhash-plugin": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
