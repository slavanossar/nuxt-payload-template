{
  "name": "nuxt3-payload-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "dotenv -- turbo run dev",
    "build": "dotenv -- turbo run build",
    "serve": "dotenv -- turbo run serve",
    "generate": "dotenv -- turbo run generate",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,md}\""
  },
  "devDependencies": {
    "prettier": "^2.8.7",
    "turbo": "^1.8.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.25.0",
  "dependencies": {
    "dotenv-cli": "^7.1.0"
  }
}