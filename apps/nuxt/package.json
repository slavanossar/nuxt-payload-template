{
  "name": "nuxt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "generate": "npm run generate:schema && npm run generate:types",
    "generate:schema": "node ./graphql/schemaQuery.mjs",
    "generate:types": "graphql-codegen -r dotenv/config --config codegen.ts && mv graphql/exports.d.ts graphql/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1"
  },
  "dependencies": {
    "@nuxtjs/apollo": "5.0.0-alpha.15",
    "@nuxtjs/seo": "^3.2.2",
    "@payloadcms/live-preview": "^3.63.0",
    "@pinia/nuxt": "0.11.3",
    "@tailwindcss/vite": "^4.1.17",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "detect-it": "^4.0.1",
    "dotenv": "^17.2.3",
    "motion-v": "^1.7.4",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "slate": "^0.118.1",
    "tailwindcss": "^4.1.17"
  }
}
