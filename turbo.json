{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".output/**", "build/**", "dist/**"]
    },
    "serve": {
      "cache": false,
      "persistent": true
    },
    "generate": {
      "outputs": ["graphql/index.js", "graphql/possibleTypes.json", "types.d.ts"]
    },
    "lint": {},
    "format": {}
  }
}
